{
  "name": "python-helper-project",
  "displayName": "Python helper project",
  "description": "",
  "version": "0.2.3",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python-helper-project.test",
        "title": "Test cmd"
      }
    ],
    "configuration": {
      "type": "object",
      "properties": {
        "typing-assist.enableCursorDownAfterCommentLine": {
          "type": "boolean",
          "default": true,
          "description": "Enable automatic commenting feature."
        },
        "typing-assist.docstringFormat": {
          "type": "string",
          "default": "NumPy",
          "enum": [
            "Plain",
            "NumPy",
            "reStructuredText",
            "Google",
            "Epytext"
          ],
          "description": "Select standard docstring format"
        },
        "typing-assist.assistOptions": {
          "type": "array",
          "default": [
            "stringSeparator",
            "docstringCompleter",
            "functionCompleter",
            "bracketingExpressionCompleter",
            "newlineSpaceRemover"
          ],
          "uniqueItems": true,
          "items": {
            "type": "string",
            "enum": [
              "stringSeparator",
              "docstringCompleter",
              "functionCompleter",
              "bracketingExpressionCompleter",
              "newlineSpaceRemover"
            ]
          },
          "description": "Enable/disable assist options"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "web-tree-sitter": "^0.20.8"
  }
}
